{
	"name": "remove yt recommendations",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["script.js"]
		}],
	"browser_action":{
		"default_popup": "popup.html",
		"default_title": "Remove Youtube recommendations"
	},
	"icons": {"16": "16 by 16 dark hole.png",
			"48": "48 by 48 dark hole.png",
			"128": "128 by 128 dark hole.png"
	}

}